@model Team
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Fullname" class="form-label">Fullname</label>
        <input asp-for=Fullname class="form-control" >
        <span asp-validation-for="Fullname" style="color:red"></span>
    </div>
    <div class="form-group">
        <label asp-for="Position" class="form-label">Position</label>
        <input asp-for=Position class="form-control" >
        <span asp-validation-for="Position" style="color:red"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description" class="form-label">Description</label>
        <input asp-for=Description class="form-control">
        <span asp-validation-for="Description" style="color:red"></span>
    </div>
    <div class="form-group">
        <label asp-for="ImgFile" class="form-label">ImgFile</label>
        <input asp-for=ImgFile class="form-control">
        <span asp-validation-for="ImgFile" style="color:red"></span>
    </div>
   
    <button type="submit" class="btn btn-primary">Submit</button>
</form>